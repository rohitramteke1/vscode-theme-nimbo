{
    "$schema": "vscode://schemas/color-theme",
    "name": "Nimbo Solarized Dark",
    "type": "dark",
    "colors": {
      "editor.background": "#002b36",
      "editor.foreground": "#93a1a1",
  
      "sideBar.background": "#002b36",
      "sideBar.foreground": "#93a1a1",
      "sideBar.border": "#002b36",
      "sideBarSectionHeader.background": "#073642",
      "sideBarSectionHeader.foreground": "#839496",
      "sideBarSectionHeader.border": "#002b36",
  
      "activityBar.background": "#002b36",
      "activityBar.foreground": "#93a1a1",
      "activityBar.border": "#002b36",
      "activityBarBadge.background": "#b58900",
      "activityBarBadge.foreground": "#002b36",
  
      "titleBar.activeBackground": "#002b36",
      "titleBar.activeForeground": "#93a1a1",
      "titleBar.inactiveBackground": "#073642",
      "titleBar.inactiveForeground": "#586e75",
      "titleBar.border": "#002b36",
  
      "statusBar.background": "#002b36",
      "statusBar.foreground": "#93a1a1",
      "statusBar.border": "#002b36",
  
      "panel.background": "#002b36",
      "panel.border": "#002b36",
      "panelTitle.activeForeground": "#b58900",
  
      "tab.activeBackground": "#002b36",
      "tab.inactiveBackground": "#073642",
      "tab.activeForeground": "#b58900",
      "tab.inactiveForeground": "#586e75",
      "tab.border": "#002b36",
  
      "editorGroup.border": "#002b36",
      "editorGroupHeader.tabsBackground": "#002b36",
      "editorGroupHeader.tabsBorder": "#002b36",
  
      "editorLineNumber.activeForeground": "#b58900",
      "editorLineNumber.foreground": "#586e75",
  
      "list.activeSelectionBackground": "#073642",
      "list.activeSelectionForeground": "#b58900",
      "list.inactiveSelectionBackground": "#002b36",
      "list.inactiveSelectionForeground": "#93a1a1",
      "list.hoverBackground": "#073642",
      "list.hoverForeground": "#b58900",
  
      "menu.background": "#002b36",
      "menu.foreground": "#93a1a1",
      "menu.selectionBackground": "#073642",
      "menu.selectionForeground": "#b58900",
      "menu.separatorBackground": "#002b36",
      "menu.border": "#002b36",
  
      "editorWidget.background": "#073642",
      "editorWidget.border": "#002b36",
  
      "peekViewEditor.background": "#073642",
      "peekViewEditor.matchHighlightBackground": "#b5890044",
  
      "terminal.background": "#002b36",
      "terminal.foreground": "#93a1a1",
  
      "quickInput.background": "#002b36",
      "quickInput.foreground": "#93a1a1",
  
      "dropdown.background": "#002b36",
      "dropdown.border": "#002b36",
      "dropdown.foreground": "#93a1a1",
  
      "input.background": "#073642",
      "input.border": "#002b36",
      "input.foreground": "#93a1a1",
      "input.placeholderForeground": "#586e75",
  
      "focusBorder": "#b58900",
      "selection.background": "#073642",
  
      "widget.shadow": "#00000000",
      "scrollbar.shadow": "#00000000",
  
      "minimap.background": "#002b36",
      "minimapSlider.background": "#07364277",
      "minimapSlider.hoverBackground": "#b5890044",
      "minimapSlider.activeBackground": "#b5890044",
  
      // Hide indent/ruler lines
    //   "editorIndentGuide.background": "#002b36",
    //   "editorIndentGuide.activeBackground": "#073642",
      "editorRuler.foreground": "#073642"
    },
    "tokenColors": [
      {
        "scope": [
          "meta.embedded",
          "source.groovy.embedded",
          "string meta.image.inline.markdown",
          "variable.legacy.builtin.python"
        ],
        "settings": { "foreground": "#839496" }
      },
      { "scope": "emphasis", "settings": { "fontStyle": "italic" } },
      { "scope": "strong", "settings": { "fontStyle": "bold" } },
      { "scope": "header", "settings": { "foreground": "#cb4b16" } },
      { "scope": "comment", "settings": { "foreground": "#586e75", "fontStyle": "italic" } },
      { "scope": "constant.language", "settings": { "foreground": "#268bd2" } },
      {
        "scope": [
          "constant.numeric",
          "variable.other.enummember",
          "keyword.operator.plus.exponent",
          "keyword.operator.minus.exponent"
        ],
        "settings": { "foreground": "#d33682" }
      },
      { "scope": "constant.regexp", "settings": { "foreground": "#2aa198" } },
      { "scope": "entity.name.tag", "settings": { "foreground": "#859900" } },
      {
        "scope": ["entity.name.tag.css", "entity.name.tag.less"],
        "settings": { "foreground": "#b58900" }
      },
      {
        "scope": "entity.other.attribute-name",
        "settings": { "foreground": "#2aa198" }
      },
      {
        "scope": [
          "entity.other.attribute-name.class.css",
          "source.css entity.other.attribute-name.class",
          "entity.other.attribute-name.id.css",
          "entity.other.attribute-name.parent-selector.css",
          "entity.other.attribute-name.parent.less",
          "source.css entity.other.attribute-name.pseudo-class",
          "entity.other.attribute-name.pseudo-element.css",
          "source.css.less entity.other.attribute-name.id",
          "entity.other.attribute-name.scss"
        ],
        "settings": { "foreground": "#b58900" }
      },
      { "scope": "invalid", "settings": { "foreground": "#dc322f" } },
      { "scope": "markup.underline", "settings": { "fontStyle": "underline" } },
      {
        "scope": "markup.bold",
        "settings": { "fontStyle": "bold", "foreground": "#b58900" }
      },
      {
        "scope": "markup.heading",
        "settings": { "fontStyle": "bold", "foreground": "#b58900" }
      },
      { "scope": "markup.italic", "settings": { "fontStyle": "italic", "foreground": "#268bd2" } },
      {
        "scope": "markup.strikethrough",
        "settings": { "fontStyle": "strikethrough", "foreground": "#b58900" }
      },
      { "scope": "markup.inserted", "settings": { "foreground": "#859900" } },
      { "scope": "markup.deleted", "settings": { "foreground": "#dc322f" } },
      { "scope": "markup.changed", "settings": { "foreground": "#b58900" } },
      {
        "scope": "punctuation.definition.quote.begin.markdown",
        "settings": { "foreground": "#586e75" }
      },
      {
        "scope": "punctuation.definition.list.begin.markdown",
        "settings": { "foreground": "#cb4b16" }
      },
      { "scope": "markup.inline.raw", "settings": { "foreground": "#268bd2" } },
      {
        "name": "brackets of XML/HTML tags",
        "scope": "punctuation.definition.tag",
        "settings": { "foreground": "#839496" }
      },
      {
        "scope": ["meta.preprocessor", "entity.name.function.preprocessor"],
        "settings": { "foreground": "#859900" }
      },
      {
        "scope": "meta.preprocessor.string",
        "settings": { "foreground": "#2aa198" }
      },
      {
        "scope": "meta.preprocessor.numeric",
        "settings": { "foreground": "#d33682" }
      },
      {
        "scope": "meta.structure.dictionary.key.python",
        "settings": { "foreground": "#b58900" }
      },
      { "scope": "meta.diff.header", "settings": { "foreground": "#b58900" } },
      { "scope": "storage", "settings": { "foreground": "#268bd2" } },
      { "scope": "storage.type", "settings": { "foreground": "#b58900" } },
      {
        "scope": ["storage.modifier", "keyword.operator.noexcept"],
        "settings": { "foreground": "#6c71c4" }
      },
      {
        "scope": ["string", "meta.embedded.assembly"],
        "settings": { "foreground": "#2aa198" }
      },
      { "scope": "string.tag", "settings": { "foreground": "#2aa198" } },
      { "scope": "string.value", "settings": { "foreground": "#2aa198" } },
      { "scope": "string.regexp", "settings": { "foreground": "#2aa198" } },
      {
        "name": "String interpolation",
        "scope": [
          "punctuation.definition.template-expression.begin",
          "punctuation.definition.template-expression.end",
          "punctuation.section.embedded"
        ],
        "settings": { "foreground": "#268bd2" }
      },
      {
        "name": "Reset JavaScript string interpolation expression",
        "scope": ["meta.template.expression"],
        "settings": { "foreground": "#93a1a1" }
      },
      {
        "scope": [
          "support.type.vendored.property-name",
          "support.type.property-name",
          "source.css variable",
          "source.coffee.embedded"
        ],
        "settings": { "foreground": "#b58900" }
      },
      { "scope": "keyword", "settings": { "foreground": "#268bd2" } },
      { "scope": "keyword.control", "settings": { "foreground": "#859900" } },
      { "scope": "keyword.operator", "settings": { "foreground": "#6c71c4" } },
      {
        "scope": [
          "keyword.operator.new",
          "keyword.operator.expression",
          "keyword.operator.cast",
          "keyword.operator.sizeof",
          "keyword.operator.alignof",
          "keyword.operator.typeid",
          "keyword.operator.alignas",
          "keyword.operator.instanceof",
          "keyword.operator.logical.python",
          "keyword.operator.wordlike"
        ],
        "settings": { "foreground": "#6c71c4" }
      },
      { "scope": "keyword.other.unit", "settings": { "foreground": "#b58900" } },
      {
        "scope": [
          "punctuation.section.embedded.begin.php",
          "punctuation.section.embedded.end.php"
        ],
        "settings": { "foreground": "#268bd2" }
      },
      {
        "scope": "support.function.git-rebase",
        "settings": { "foreground": "#859900" }
      },
      {
        "scope": "constant.sha.git-rebase",
        "settings": { "foreground": "#d33682" }
      },
      {
        "name": "coloring of the Java import and package identifiers",
        "scope": [
          "storage.modifier.import.java",
          "variable.language.wildcard.java",
          "storage.modifier.package.java"
        ],
        "settings": { "foreground": "#93a1a1" }
      },
      {
        "name": "this.self",
        "scope": "variable.language",
        "settings": { "foreground": "#b58900" }
      }
    ],
    "semanticHighlighting": false
  }
  